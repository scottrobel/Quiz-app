<div><%# link_to "#{@user.email}", user_path(@user.id)%></div>
<% @question_answer_pairs.each do |question, answer_array| %>
  <h4><%= question.question_type.split('_').join(' ') %></h4>
  <h3><%= question.contents %></h3>
  <ul>
    <% answer_array.each do |answer| %>
      <li><%= answer.contents %></li>
    <% end %>
  </ul>
<% end %>