<div class="top-message"><%= link_to "<< Other Responses", quiz_responses_path(@response.quiz.id) if current_user.admin_user? %></div>
<h1 class="top-message">Quiz: <%= @response.quiz.title %></h1>
<div class="top-message">User: <%= link_to "#{@user.email}", user_path(@user.id)%></div>
<% @question_answer_pairs.each do |question, answer_array| %>
  <h3 class="top-message">Question Title: <%= question.contents %>   Question Type: <%= question.question_type.split('_').join(' ') %></h3>
  <% answer_array.each do |answer| %>
    <div class="top-message"><%= answer.contents %></div>
  <% end %>
<% end %>